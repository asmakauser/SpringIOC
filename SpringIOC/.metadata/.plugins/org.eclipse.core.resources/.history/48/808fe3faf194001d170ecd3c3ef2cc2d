package com.springcore;

public class Calculate {

	private int a;
	private int b;
	
	public Calculate(int a, int b) {
		super();
		this.a = a;
		this.b = b;
		System.out.println("Constructor: int, int");
	}
	
	
	public Calculate(double a, double b) {
		super();
		this.a = (int) a;
		this.b = (int) b;
		System.out.println("Constructor: double, double");
	}
	
	
	
}
